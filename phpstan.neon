includes:
	- phpstan-baseline.neon
	- vendor/phpstan/phpstan/conf/bleedingEdge.neon

parameters:
	level: 0
	paths:
		- includes
		- languages
		- tests/phpunit
	excludePaths:
	scanDirectories:
