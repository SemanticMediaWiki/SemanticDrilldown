includes:
	- phpstan-baseline.neon

parameters:
	level: 1
	paths:
	    - .
	excludePaths:
	    - ./vendor
	    - ./node_modules
	    - ./coverage
	scanDirectories:
		- ../SemanticMediaWiki/includes
		- ../SemanticMediaWiki/src/Query
		- ../SemanticMediaWiki/tests/phpunit
		- ../PageSchemas/includes
		- ../AdminLinks/includes
		- ../../includes
		- ../../languages
		- ../../tests/phpunit
		- ../../vendor
